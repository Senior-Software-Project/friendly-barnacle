{
  "name": "alarm-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "fixStyle": "npx eslint --fix . --ext .js",
    "checkStyle": "npx eslint . --ext .js",
    "test": "jest --watch --coverage=false",
    "coverage": "jest --collectCoverage --",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^6.1.0",
    "@react-navigation/bottom-tabs": "^6.2.0",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@testing-library/react-native": "^9.0.0",
    "expo": "^44.0.0",
    "expo-app-loading": "~1.3.0",
    "expo-cli": "^4.13.0",
    "expo-file-system": "^13.1.4",
    "expo-modules-core": "^0.6.5",
    "expo-splash-screen": "~0.14.1",
    "expo-status-bar": "~1.2.0",
    "expo-storage": "^44.0.0",
    "expo-updates": "~0.11.6",
    "html-entities": "^2.3.2",
    "prop-types": "^15.8.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-alarm-manager": "^1.2.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-web": "0.17.1",
    "react-navigation": "^4.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@testing-library/jest-dom": "^5.16.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.29.2",
    "jest": "^26.6.3",
    "jest-expo": "^44.0.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "node-fetch": "^3.2.3",
    "prettier": "2.5.1",
    "prettier-standard": "^13.0.6",
    "react-test-renderer": "17.0.1",
    "standard": "^16.0.4"
  },
  "private": true,
  "jest": {
    "automock": false,
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "testURL": "http://localhost/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ]
  }
}
